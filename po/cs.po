#: vanilla_installer/gtk/default-users.ui:58
msgid "Password"
msgstr "Heslo"

#: vanilla_installer/gtk/default-users.ui:63
msgid "Confirm Password"
msgstr "Potvrzení hesla"

#: vanilla_installer/gtk/default-welcome.ui:11
msgid "Welcome"
msgstr "Vítejte"

#: vanilla_installer/gtk/default-welcome.ui:12
msgid "This software will guide you through the installation process."
msgstr "Tento program vás provede procesem instalace Vanilla OS."

#: vanilla_installer/gtk/default-welcome.ui:41
msgid "Try Vanilla OS"
msgstr "Vyzkoušet Vanilla OS"

#: vanilla_installer/gtk/default-welcome.ui:76
#: vanilla_installer/gtk/dialog-confirm.ui:35
msgid "Install Vanilla OS"
msgstr "Nainstalovat Vanilla OS"

#: vanilla_installer/gtk/dialog-confirm.ui:6
#: vanilla_installer/gtk/dialog-confirm.ui:24
msgid "Confirm Installation"
msgstr "Potvrďte instalaci"

#: vanilla_installer/gtk/dialog-confirm.ui:17
#: vanilla_installer/gtk/dialog-disk-confirm.ui:18
#: vanilla_installer/gtk/dialog-disk.ui:18
msgid "Cancel"
msgstr "Zrušit"

#: vanilla_installer/gtk/dialog-confirm.ui:25
msgid "The following changes will be made to your system:"
msgstr "V systému budou provedeny následující změny:"

#: vanilla_installer/gtk/dialog-disk-confirm.ui:6
#: vanilla_installer/gtk/dialog-disk-confirm.ui:25
#: vanilla_installer/gtk/dialog-disk-confirm.ui:36
msgid "Confirm Changes"
msgstr "Potvrdit změny"

#: vanilla_installer/gtk/dialog-disk-confirm.ui:26
msgid ""
"The following changes will be made to your disk. Please review them "
"carefully."
msgstr "Na disku budou provedeny následující změny. Pečlivě si je prohlédněte."

#: vanilla_installer/gtk/dialog-disk.ui:6
msgid "Configure Disk"
msgstr "Konfigurace disku"

#: vanilla_installer/gtk/dialog-disk.ui:23
msgid "Apply"
msgstr "Použít"

#: vanilla_installer/gtk/dialog-disk.ui:37
msgid "Use Entire Disk"
msgstr "Použít celý disk"

#: vanilla_installer/gtk/dialog-disk.ui:38
msgid "This will erase all data on the disk."
msgstr "Tím se vymažou všechna data na disku."

#: vanilla_installer/gtk/dialog-disk.ui:59
msgid "Open GParted"
msgstr "Otevřít aplikaci GParted"

#: vanilla_installer/gtk/dialog-disk.ui:60
msgid "Open GParted to manually partition the disk"
msgstr "Otevření aplikace GParted pro ruční rozdělení disku"

#: vanilla_installer/gtk/dialog-disk.ui:81
msgid "Partitions"
msgstr "Oddíly"

#: vanilla_installer/gtk/dialog-disk.ui:82
msgid "The following partitions were found on the disk"
msgstr "Na disku byly nalezeny následující oddíly"

#: vanilla_installer/gtk/dialog.ui:6
msgid "Showing Information"
msgstr "Zobrazení informací"

#: vanilla_installer/gtk/done.ui:12
msgid "All done!"
msgstr "Hotovo!"

#: vanilla_installer/gtk/done.ui:13
msgid "Restart your device to enjoy your Vanilla OS experience"
msgstr ""
"Restartujte zařízení, abyste si mohli užívat operačního systému Vanilla OS"

#: vanilla_installer/gtk/done.ui:22
msgid "Reboot Now"
msgstr "Restartovat nyní"

#: vanilla_installer/gtk/done.ui:32
msgid "Close"
msgstr "Zavřít"

#: vanilla_installer/gtk/layout-yes-no.ui:36
msgid "Yes, enable it"
msgstr "Ano, povolit to"

#: vanilla_installer/gtk/progress.ui:60
msgid "Installing…"
msgstr "Instaluji…"

#: vanilla_installer/gtk/progress.ui:81
msgid "Show Console output"
msgstr "Ukázat výstup z konzole"

#: vanilla_installer/gtk/progress.ui:96
#, fuzzy
msgid "Show Slideshow"
msgstr "Zobrazit prezentaci"

#: vanilla_installer/gtk/widget-partition.ui:10
msgid "Format"
msgstr "Formát"

#: vanilla_installer/gtk/widget-partition.ui:11
msgid "Choose a new file system or leave as is"
msgstr "Zvolte nový souborový systém nebo jej ponechte tak, jak je"

#: vanilla_installer/gtk/widget-partition.ui:29
msgid "Mount Point"
msgstr "Bod připojení"

#: vanilla_installer/gtk/widget-partition.ui:30
msgid "Choose a mount point for the partition"
msgstr "Vyberte bod připojení pro diskový oddíl"

#: vanilla_installer/defaults/disk.py:46
msgid "This is the only disk available and cannot be deselected!"
msgstr "Toto je jediný rozpoznaný disk a nemůže být odznačen!"

#: vanilla_installer/defaults/disk.py:190
msgid "Entire disk will be used."
msgstr "Bude použit celý disk."

#: vanilla_installer/defaults/disk.py:195
msgid "Will be formatted in {} and mounted in {}"
msgstr "Bude naformátován v {} a připojen v {}"

#: vanilla_installer/layouts/preferences.py:68
msgid "Please select at least one option."
msgstr "Vyberte prosím alespoň jednu možnost."

#: vanilla_installer/views/done.py:34
msgid "Restart your device to enjoy your {} experience."
msgstr "Restartujte zařízení, abyste si mohli vychutnat své {} zážitky."

#: vanilla_installer/views/done.py:45
msgid "Something went wrong"
msgstr "Něco se pokazilo"

#: vanilla_installer/views/done.py:46
msgid "Please contact the distribution developers."
msgstr "Obraťte se prosím na vývojáře distribuce."
